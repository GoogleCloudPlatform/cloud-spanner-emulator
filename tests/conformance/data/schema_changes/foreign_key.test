  A INT64 NOT NULL,
  B INT64 NOT NULL,
) PRIMARY KEY(A, B);
CREATE TABLE U (
  X INT64 NOT NULL,
  Y INT64 NOT NULL,
  FOREIGN KEY(X, Y) REFERENCES T(B, A),
  FOREIGN KEY(X, Y) REFERENCES T(B, A),
  CONSTRAINT D FOREIGN KEY(Y) REFERENCES T(B),
) PRIMARY KEY(X, Y);
DROP TABLE U;
--
CREATE TABLE T (
  A INT64 NOT NULL,
  B INT64 NOT NULL,
) PRIMARY KEY(A, B);
==
