# Can drop a column when its not used by the view.
CREATE TABLE t (c STRING(MAX), c1 INT64) PRIMARY KEY ();
CREATE VIEW foo SQL SECURITY INVOKER
  AS SELECT t.c1 AS a FROM t;
ALTER TABLE t DROP COLUMN c;
--
