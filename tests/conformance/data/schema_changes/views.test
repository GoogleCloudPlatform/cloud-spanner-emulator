# Create view with same name as built-in function.
CREATE TABLE T (
  K INT64 NOT NULL,
) PRIMARY KEY(K);
CREATE VIEW MOD SQL SECURITY INVOKER AS
  SELECT T.K FROM T WHERE MOD(T.K, 5) = 0;
CREATE VIEW V SQL SECURITY INVOKER AS
  SELECT T.K FROM T WHERE MOD(T.K, 6) = 0;
--
CREATE TABLE T ( K INT64 NOT NULL, ) PRIMARY KEY(K);
CREATE VIEW MOD SQL SECURITY INVOKER AS SELECT T.K FROM T WHERE MOD(T.K, 5) = 0;
CREATE VIEW V SQL SECURITY INVOKER AS SELECT T.K FROM T WHERE MOD(T.K, 6) = 0;
