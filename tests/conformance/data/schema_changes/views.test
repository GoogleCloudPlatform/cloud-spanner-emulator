# Cannot drop a table column used by a view.
# --regex
CREATE TABLE t (c STRING(MAX)) PRIMARY KEY ();
CREATE VIEW foo
  SQL SECURITY INVOKER AS SELECT t.c FROM t;
ALTER TABLE t DROP COLUMN c;
